
<div class="container">

  <div class="row align-items-center my-3 py-3">
      <h4>会員一覧</h4>
  </div>

    <table class="table">


      <thead class="table-secondary">
        <tr>
          <th>会員ID</th>
          <th>氏名</th>
          <th>メールアドレス</th>
          <th>ステータス</th>
        </tr>
      </thead>

 <% @customers.each do |customer| %>
      <tbody>
        <tr>
          <th><%= customer.id %></th>
          <th><%= link_to "customer.last_name", admin_customer_path(customer.id) %><%= link_to "customer.first_name", admin_customer_path(customer.id) %></th>
          <th><%= customer.email %></th>
          <th>ステータス</th>
      </tbody>
 <% end %>
    </table>



</div>
