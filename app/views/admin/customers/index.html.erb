<h1>会員一覧</h1>
<%= @customers.each do |customer| %>
<table>

  <thead>
    <tr>
      <td>会員ID</td>
       <td>氏名</td>
        <td>メールアドレス</td>
         <td>ステータス</td>
    </tr>
  </thead>

  <tbody>
    <tr>
       <td><%= customer.id %></td>
       <td><%= customer.last_name %><%= customer.first_name %></td>
        <td><%= customer.email %></td>
         <td>ステータス</td>
    </tr>
  </tbody>
</table>
<% end %>